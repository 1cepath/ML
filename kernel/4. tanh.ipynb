   ],
   "source": [
    "import math\n",
    "print(math.tanh(-0.10))\n",
    "print(math.tanh(0))\n",
    "print(math.tanh(0.17))\n",
    "print(math.tanh(332))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def tanh(x):\n",
    "  return (math.exp(x) - math.exp(-x)) / (math.exp(x) + math.exp(-x))\n",
    "\n"
   ]
  },
